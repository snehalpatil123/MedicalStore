@{
}
<table class="table table-bordered table-hover">
    <tr class="text-center bg-dark text-white">
        <th>Receipt Id</th>
        <th>Receipt Date</th>
        <th>Customer Name</th>
         <th>Receipt ammount</th>
       

    </tr>
    @{
        if (ViewData["list"] != null)
        {
            List<dynamic> Receiptlist = (List<dynamic>)ViewData["list"];
            foreach (var Receiptdata in Receiptlist)
            {
                <tr class="text-center">
                    <td>@Receiptdata.rec_id</td>
                  <td>@Receiptdata.rec_date</td>
                  <td>@Receiptdata.cust_nm</td>
                  <td>@Receiptdata.rec_amt</td>


                </tr>
            }
        }
    }
</table>