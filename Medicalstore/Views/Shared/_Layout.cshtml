<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Medicalstore</title>
    <!-- Favicon -->
    <link href="~/medical-store/img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Jost:wght@500;600;700&family=Open+Sans:wght@400;600&display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="~/medical-store/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/medical-store/lib/animate/animate.min.css" rel="stylesheet">
    <link href="~/medical-store/lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css" rel="stylesheet" />
    <link href="~/medical-store/lib/twentytwenty/twentytwenty.css" rel="stylesheet" />

    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/medical-store/css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="~/medical-store/css/style.css" rel="stylesheet">


</head>

<script>
    function printInvoice()
    {
        document.getElementById("print").style.opacity = 0;
        var page =document.getElementById("render")
        page.style.fontSize="10px"
        window.print();
        document.getElementById("print").style.opacity = 1;
        page.style.fontSize = "20px"
    }
</script>
<body>
    <!-- Spinner Start -->
    <div id="spinner" class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
        <div class="spinner-grow text-primary m-1" role="status">
            <span class="sr-only">Loading...</span>
        </div>
        <div class="spinner-grow text-dark m-1" role="status">
            <span class="sr-only">Loading...</span>
        </div>
        <div class="spinner-grow text-secondary m-1" role="status">
            <span class="sr-only">Loading...</span>
        </div>
    </div>
    <!-- Spinner End -->
    <header>
        <nav class="navbar navbar-expand-lg bg-white navbar-light shadow-sm px-5 py-3 py-lg-0" style="z-index:10;">
            <a href="/" class="navbar-brand p-0">
                <h1 class="m-0 text-primary"><i class="fa fa-capsules me-2"></i>MediCare</h1>
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarCollapse">
                <div class="navbar-nav ms-auto py-0">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>

                    @* <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a> *@

                    <div class="nav-item dropdown">
                        <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Manage</a>
                        <div class="dropdown-menu m-0">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Brand" asp-action="Index">Brand</a>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Medicine_Cat" asp-action="Index">Medicine Category</a>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Medicine_Master" asp-action="Index">Medicine Master</a>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Customer" asp-action="Index">Customer</a>
                        </div>
                    </div>
                            <div class="nav-item dropdown">
                                <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Transactions</a>
                                <div class="dropdown-menu m-0">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Sale_master" asp-action="Index">Sale Master</a>
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Sale_details" asp-action="Index">Sale Details</a>
                                   @*  <a class="nav-link text-dark" asp-area="" asp-controller="Receipt" asp-action="Index">Receipt</a>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Invoice" asp-action="Index">ShowRecords</a> *@
                                    </div>
                                 </div>
                                    <div class="nav-item dropdown">
                                        <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">List Reports</a>
                        <div class="dropdown-menu m-0">

                            <a class="nav-link text-dark" asp-area="" asp-controller="BrandReport" asp-action="BrandlistView">BrandReport</a>
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="Medicine_MasterReport" asp-action="Medicine_MasterlistView">Medicine Master Report</a>
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="Sale_masterReport" asp-action="Sale_masterlistView">Sale Master Report</a>
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="Sale_detailsReport" asp-action="Sale_detailslistView">SaleDetailsReport</a>
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="CustomerReport" asp-action="CustomerlistView">CustomerReport</a>
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="Sale_detailsmasterReport" asp-action="Sale_detailsmasterlistView">SaleDetailsmasterReport</a>
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="ReceiptReport" asp-action="ReceiptlistView">ReceiptReport</a>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Medicine_CatReport" asp-action="Medicine_CatlistView">Medicine Category Report</a>



                        </div>
                    </div>
                    <div class="nav-item dropdown">
                        <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Dynamic Reports</a>
                        <div class="dropdown-menu m-0">
                            @* <a class="nav-link text-dark" asp-area="" asp-controller="Medicine_MasterbrandReport" asp-action="Medicine_MasterbrandlistView">Medicine Master BrandReport</a> *@
                            <a class="nav-link text-dark" asp-area="" asp-controller="Medicine_MasterReport" asp-action="Medicine_CatwiseMedicine_MasterView">Medicine Category wise Medicine Master</a>


                            <a class="nav-link text-dark" asp-area="" asp-controller="Medicine_MasterbrandReport" asp-action="BrandwiseMedicine_MasterView">Brand wise MedicineMaster</a>



                            <a class="nav-link text-dark" asp-area="" asp-controller="Sale_masterReport" asp-action="CustomerwiseSale_masterView">Customer Wise SaleMasterReport</a>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Sale_detailsReport" asp-action="Medicine_MasterwiseSale_detailsView">Medicine Masterwise Saledetails</a>


                            <a class="nav-link text-dark" asp-area="" asp-controller="Sale_detailsmasterReport" asp-action="Sale_masterwiseSale_detailsmasterView">Sale Masterwise Saledetails</a>





                            <a class="nav-link text-dark" asp-area="" asp-controller="ReceiptReport" asp-action="CustomerwiseReceiptView">CustomerWiseReceiptReport</a>

                        </div>
                    </div>
                    <div class="nav-item dropdown">
                        <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Date Wise Reports</a>
                        <div class="dropdown-menu m-0">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Sale_masterReport" asp-action="DateWiseSale_masterView">Date Wise SaleMasterReport</a>


                         
                          


                            <a class="nav-link text-dark" asp-area="" asp-controller="ReceiptReport" asp-action="DateWiseReceiptView">DateWiseReceiptReport</a>

                        </div>
                    </div>

                   

                           
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class="container" style="min-height:100vh;" id="render">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - MediCare Medical Store - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/medical-store/lib/wow/wow.min.js"></script>
    <script src="~/medical-store/lib/easing/easing.min.js"></script>
    <script src="~/medical-store/lib/waypoints/waypoints.min.js"></script>
    <script src="~/medical-store/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/medical-store/lib/tempusdominus/js/moment.min.js"></script>
    <script src="~/medical-store/lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="~/medical-store/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>
    <script src="~/medical-store/lib/twentytwenty/jquery.event.move.js"></script>
    <script src="~/medical-store/lib/twentytwenty/jquery.twentytwenty.js"></script>

    <!-- Template Javascript -->
    <script src="~/medical-store/js/main.js"></script>
</body>
</html>
