@{
}
<table class="table  table-bordered table-hover">
    <tr class="text-center bg-dark text-white">
        <th>Medicine Category Id</th>
        <th>Medicinen Category Name</th>

    </tr>
    @{
        if (ViewData["medicine_Cats"] != null)
        {
            List<Medicine_Cat> medicinelist = (List<Medicine_Cat>)ViewData["medicine_Cats"];
            foreach (var medicine in medicinelist)
            {
                <tr class="text-center">
                    <td>@medicine.cat_id</td>
                    <td>@medicine.cat_nm</td>

                </tr>
            }
        }
    }
</table>
