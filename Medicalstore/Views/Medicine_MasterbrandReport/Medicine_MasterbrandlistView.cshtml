@{
}
<table class="table table-bordered table-hover">
    <tr class="text-center text-white bg-dark">
        <th>Medicine Id</th>
        <th>Medicine Name</th>
        <th>Category Name</th>
        <th>Brand Name</th>
        <th>Contents</th>
        <th>Rate</th>
        <th>Stock</th>


    </tr>
    @{
        if (ViewData["medicine_Masterslist"] != null)
        {
            List<dynamic> Medicinelist = (List<dynamic>)ViewData["medicine_Masterslist"];
            foreach (var Medicinedata in Medicinelist)
            {
                <tr class="text-center">
                    <td>@Medicinedata.med_id</td>
                    <td>@Medicinedata.med_nm</td>
                    <td>@Medicinedata.cat_nm</td>
                    <td>@Medicinedata.brand_nm</td>
                    <td>@Medicinedata.contents</td>
                    <td>@Medicinedata.rate</td>
                    <td>@Medicinedata.stock</td>


                </tr>
            }
        }
    }
</table>
